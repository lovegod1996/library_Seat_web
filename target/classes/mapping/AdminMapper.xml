<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.xoqao.web.dao.AdminMapper" >
    <!--根据用户名或账号查询管理员-->
    <select id="findAdminByCount" resultType="com.xoqao.web.bean.admin.Admin">
        SELECT aid,acountnumber,password,name,employer  FROM admin WHERE acountnumber LIKE '${loginstr}'
    </select>

    <!--根据id查找管理员-->
    <select id="findAdminByid" resultType="com.xoqao.web.bean.admin.Admin">
          SELECT aid,acountnumber,password,name,employer  FROM admin WHERE aid =#{aid}
    </select>
</mapper>