<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.xoqao.web.dao.FloorsMapper">
    <!--根据用户名或账号查询管理员-->
    <select id="findFloorAdminByCount" resultType="com.xoqao.web.bean.floors.Floor">
        SELECT fid,bid,accountnumber,password,name,employer,floor  FROM floor WHERE accountnumber LIKE '${loginstr}'
    </select>
    <!--根据id查询-->
    <select id="findfloorByid" resultType="com.xoqao.web.bean.floors.Floor">
           SELECT fid,bid,accountnumber,password,name,employer,floor  FROM floor WHERE fid=#{fid}
    </select>

</mapper>